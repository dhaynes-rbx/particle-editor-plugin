export type Style = {
    ATTENTION: "string",
    TUTORIAL: "string",
    DEFAULT: "string",
}

local STYLE: Style = {
    ATTENTION = "Attention",
    TUTORIAL = "Tutorial",
    DEFAULT = "Default",
}
--- Module

--[[--
    @tfield number? Step
    @tfield boolean? Current
    @tfield boolean? Interactive
    @tfield boolean? Focused
    @tfield Style? Style
    @tfield boolean? Disabled
    @tfield (nil) -> (nil))? OnActivated
