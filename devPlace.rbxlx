    local theme = React.useContext(ThemeContext)

    local children = {}

    local topBarInsetHeight = if topBarInset.Height == TOPBAR_FALLBACK then topBarInset.Height else TOPBAR_FALLBACK

    if props.TopBarProps then
        props.TopBarProps["PaddingLeft"] = topBarInset.Min.X - initialTopBarInsetMinX
    end

    children["TopBar"] = Row({
        Size = UDim2.new(1, 0, 0, topBarInsetHeight),
        PaddingHorizontal = topBarInset.Min.X,
