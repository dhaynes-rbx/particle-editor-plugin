export type ProgressTrackStepProps = {
    Step: number?,
    Current: boolean?,
    Interactive: boolean?,
    Focused: boolean?,
    Style: Style?,
    Disabled: boolean?,
    OnActivated: (nil) -> nil?,
    ZIndex: number?,
    HasTutorialOffset: true,
