        TopBarContent = Block({
            Size = UDim2.new(1, 0, 0, topBarInsetHeight),
        }, {
            TopBar = TopBar(props.TopBarProps),
        }),
    })

    children["Task"] = Block({
        Size = UDim2.new(1, 0, 1, if props.Minimal then 0 else -topBarInsetHeight),
        Position = UDim2.fromOffset(0, if props.Minimal then 0 else topBarInsetHeight),
    }, {
        Task = props.Task,
        HelpMenu = props.HelpMenuContent,
        Dialog = props.Dialog,
    })

    return Block({
        Size = UDim2.fromScale(1, 1),
    }, children)
end

return function(props: Props)
    return React.createElement(TaskContainer, props)
end
