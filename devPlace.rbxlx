        end

        -- this is the block we want to colorize on time warning
        return Block({
            Size = UDim2.fromOffset(0, 0),
            AutomaticSize = Enum.AutomaticSize.XY,
            BackgroundColor = theme.Tokens.Colors.SurfaceInverted.Color,
