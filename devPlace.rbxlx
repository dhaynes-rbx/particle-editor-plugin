local function TextInput(props: Props, children)
    -- Hooks
    local focused, setFocused = React.useState(false)
    local scrollingFrameRef = React.useRef(nil)
    local textContainerRef = React.useRef(nil)
    local textBoxRef = React.useRef(nil)

    React.useEffect(function()
        local scrollingFrame: ScrollingFrame = scrollingFrameRef.current
        local textContainer: Frame = textContainerRef.current
        local textBox: TextBox = textBoxRef.current

        local updateTextBoxSize = function()
            local textSize = TextService:GetTextSize(
                textBox.Text,
                textBox.TextSize,
                textBox.Font.Name,
                Vector2.new(textBox.AbsoluteSize.X, math.huge)
            )

            if textSize.Y >= scrollingFrame.AbsoluteCanvasSize.Y then
                textContainer.Size = UDim2.new(textContainer.Size.Width, UDim.new(0, textSize.Y))
            elseif textSize.Y < scrollingFrame.AbsoluteSize.Y then
                textContainer.Size = UDim2.fromScale(1, 1)
            end
        end
        updateTextBoxSize()

        local textChangedConnection = textBox:GetPropertyChangedSignal("Text"):Connect(updateTextBoxSize)

        local cursorPositionChangedConnection = textBox:GetPropertyChangedSignal("CursorPosition"):Connect(function()
            local text = textBox.Text:sub(1, textBox.CursorPosition)
            local textSize = TextService:GetTextSize(
                text,
                textBox.TextSize,
                textBox.Font.Name,
                Vector2.new(textBox.AbsoluteSize.X, math.huge)
            )
            local linePosition = textBox.AbsolutePosition.Y + textSize.Y
            if
                linePosition > (scrollingFrame.AbsolutePosition.Y + scrollingFrame.AbsoluteSize.Y - textBox.TextSize)
            then
                scrollingFrame.CanvasPosition = Vector2.new(
                    scrollingFrame.CanvasPosition.X,
                    scrollingFrame.CanvasPosition.Y
                        + linePosition
                        - (scrollingFrame.AbsolutePosition.Y + scrollingFrame.AbsoluteSize.Y)
                )
            elseif linePosition - textBox.TextSize < scrollingFrame.AbsolutePosition.Y then
                scrollingFrame.CanvasPosition = Vector2.new(
                    scrollingFrame.CanvasPosition.X,
                    scrollingFrame.CanvasPosition.Y
                        + (linePosition - (scrollingFrame.AbsolutePosition.Y + textBox.TextSize))
                )
            end
