local Packages = script.parent.Parent
local Dash = require(Packages.Dash)

type Write = {
    write: (any) -> boolean,
    priority: number,
    defaultPriority: number,
    params: any,
}

export type WriteSync = {
    registerWrite: (() -> boolean, number, any) -> nil,
    unregisterWrite: (() -> boolean) -> nil,
}

local DEFAULT_INTERVAL = 15

local writes: { { Write } } = {}
local started = false
local delay = DEFAULT_INTERVAL
local failureCount = 0
local failureCountCallback: (failureCount: number) -> nil? = nil
local writeCount = 0
local writeCountCallback: (writeCount: number) -> nil? = nil

function sortWrites()
    table.sort(writes, function(left: Write, right: Write)
        return left.priority < right.priority
    end)
end

function update()
    while true do
        task.wait(delay)
        local resort = false
        local success = true

        -- Try to perform each write
        for index: number, write: Write in writes do
            -- If write fails, bump up the priority and move to the front of the queue
            if not write.write(write.params) then
                write.priority = -1
                if index > 1 then
                    table.insert(writes, 1, table.remove(writes, index))
                end
                success = false
                failureCount += 1
                if failureCountCallback then
                    failureCountCallback(failureCount)
                end
                break

                -- Flag the need to resort
            elseif write.priority ~= write.defaultPriority then
                write.priority = write.defaultPriority
                resort = true
            end
        end

        if success then
            failureCount = 0
            if resort then
                sortWrites()
            end
        end

        writeCount += 1
        if writeCountCallback then
            writeCountCallback(writeCount)
        end
    end
end

function insertWrite(write: Write)
    local inserted = false
    for i, otherWrite in writes do
        if write.priority <= otherWrite.priority then
            table.insert(writes, i, write)
            inserted = true
            break
        end
    end
    if not inserted then
        table.insert(writes, write)
    end
end

function registerWrite(write: () -> boolean, priority: number, ...)
    insertWrite({
        write = write,
        priority = priority,
        defaultPriority = priority,
        params = ...,
    })
    if not started then
        started = true
        task.spawn(update)
    end
end

function unregisterWrite(write: () -> boolean)
    local index = Dash.findIndex(writes, function(writeEntry: Write)
        return writeEntry.write == write
    end)
    if index then
        table.remove(writes, index)
    end
end

function overrideDelay(newDelay: number)
    delay = newDelay
end

function reset()
    table.clear(writes)
    delay = DEFAULT_INTERVAL
    failureCountCallback = nil
    failureCount = 0
    writeCountCallback = nil
    writeCount = 0
end

function setFailureCountCallback(callback: (failureCount: number) -> nil)
    failureCountCallback = callback
end

function setWriteCountCallback(callback: (writeCount: number) -> nil)
    writeCountCallback = callback
end

function getWriteCount(): number
    return writeCount
end

return {
    overrideDelay = overrideDelay,
    registerWrite = registerWrite,
    reset = reset,
    unregisterWrite = unregisterWrite,
    setFailureCountCallback = setFailureCountCallback,
    setWriteCountCallback = setWriteCountCallback,
    getWriteCount = getWriteCount,
}
