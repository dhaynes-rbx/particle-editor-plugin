    End: Vector3,
    ZIndex: number?,
    BackgroundColor3: Color3,
    BackgroundTransparency: number,
}

local LinePropDefaults = {
    Active = true,
    Offset = 0,
    DashHeight = 6,
    DashWidth = 5,
    DashGap = 5,
