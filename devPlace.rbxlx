        for index, value in pairs(self.props.Choices) do
            local radioButton = RadioButton({
                Label = value.Label,
                LayoutOrder = value.Order,
                Value = value.Value,
                AsColumn = self.props.AsRow,
                RefSize = self.props.RefSizes and self.props.RefSizes[index] or nil,
