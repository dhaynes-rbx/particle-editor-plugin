                local childPath = path == "" and key or path .. "." .. key
                changedPaths = Dash.append(changedPaths, findDeltaPaths(value, left[key], childPath))
            end
        end

        return changedPaths
    end

    if left == right then
        return changedPaths
    end

    table.insert(changedPaths, path)
    return changedPaths
end

return findDeltaPaths
