        end

        local radioButton = Block({
            ZIndex = self.props.ZIndex,
            Size = UDim2.fromOffset(
                theme.Tokens.Sizes.Registered.MediumPlus.Value,
                theme.Tokens.Sizes.Registered.MediumPlus.Value
            ),
            OnMouseEnter = function()
                self:setState({
                    isHovered = true,
