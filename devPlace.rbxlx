                    end,
                    [ReactRoblox.Change.Text] = function(rbx)
                        -- Don't update if this is what was passed in.
                        if rbx.Text == props.Value then
                            return
                        end
                        if textBoxRef.current then
                            -- strip out anything beyond alphanumberic characters, and punctuation
                            if #textBoxRef.current.Text > 0 then
                                textBoxRef.current.Text = string.gsub(textBoxRef.current.Text, "[^%s%w%p]+", "")
                            end
                        end
                        if props.OnChanged then
                            props.OnChanged(rbx.Text)
                        end
                    end,
                    ZIndex = props.ZIndex or 1,
                }),
            }),
        })

        local textInputWrapper = React.createElement("ImageLabel", {
            Size = props.Size,
            AutomaticSize = Enum.AutomaticSize.None,
            ClipsDescendants = true,
            Image = (function()
                if props.Disabled then
